#include <iostream> //standard i/o header file
using namespace std;
int main()
{
	int A,l,b,r;
	float C;
	cout<<"Enter lenght of square ";
	cin>>l;
	cout<<"Enter breath of square ";
	cin>>b;
	cout<<"Enter radius of circle ";
	cin>>r;
	A=l*b; // Formula of Area Of Square
	cout<<"Area of a Square "<<A<<endl;
	C=3.14*r*r; // Formula of circumference of circle
	cout<<"circumference of a circle "<<C<<endl;
	return 0;
