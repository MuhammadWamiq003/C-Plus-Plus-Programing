#include<iostream>
using namespace std;
int main()
{
	int password;
	char username;
	cout<<"Write username: ";
	cin>>username;
		if (username=='a')
		{
			cout<<"Enter Password: ";
			cin>>password;
				if (password==1234)
				{
					cout<<"Login sucessful";
				}
				else
				{
					cout<<"Incorrect Password";
				}
		}
		else
		{
			cout<<"Username is incorrect";
		}
	return 0;
}
